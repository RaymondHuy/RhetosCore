{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },

  "ServerConnectionString": {
    "Value": "Data Source=<ServerName>;Initial Catalog=<DatabaseName>;Integrated Security=SSPI;",
    "Provider": "Rhetos.MsSql"
  },

  "AppSettings": {
    "AssemblyGenerator.ErrorReportLimit": 5,
    "EntityFramework.UseDatabaseNullSemantics": "false",
    "DataMigration.SkipScriptsWithWrongOrder": "false",
    "Security.LookupClientHostname": "false",
    "AuthorizationAddUnregisteredPrincipals": "false",
    "BuiltinAdminOverride": "true"
  }
}
